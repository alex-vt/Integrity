<androidx.preference.PreferenceScreen xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory
        app:title="Included in this app"
        >

        <Preference
            app:key="extensions_blog"
            app:title="Blog type"
            app:summary="Downloads website pages, with pagination"
            />

    </PreferenceCategory>

    <PreferenceCategory
        app:key="extensions"
        app:title="Extensions (in separate apps)"
        >

        <Preference
            app:key="extensions_get"
            app:title="Get extensions..."
            />

    </PreferenceCategory>

</androidx.preference.PreferenceScreen>